<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.crm.domain.SysUserDetailEntity" table="sys_user_detail" schema="crm_ssh">
        <id name="userDetailId" column="user_detail_id"/>
        <property name="userAge" column="user_age"/>
        <property name="userAddr" column="user_addr"/>
        <property name="userResume" column="user_resume"/>
    </class>
</hibernate-mapping>